{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "virtualNetworkName": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "virtualNetworkAddressPrefix": {
      "type": "string"
    },
    "Subnet1Name": {
      "type": "string"
    },
    "Subnet1Prefix": {
      "type": "string"
    },
    "Subnet2Name": {
      "type": "string"
    },
    "Subnet2Prefix": {
      "type": "string"
    },
    "Subnet3Name": {
      "type": "string"
    },
    "Subnet3Prefix": {
      "type": "string"
    },
    "Subnet4Name": {
      "type": "string"
    },
    "Subnet4Prefix": {
      "type": "string"
    },
    "Subnet5Name": {
      "type": "string"
    },
    "Subnet5Prefix": {
      "type": "string"
    },
    "Subnet6Name": {
      "type": "string"
    },
    "Subnet6Prefix": {
      "type": "string"
    },
    "Subnet7Name": {
      "type": "string"
    },
    "Subnet7Prefix": {
      "type": "string"
    },
    "Subnet8Name": {
      "type": "string"
    },
    "Subnet8Prefix": {
      "type": "string"
    },
    "vnetExistingRGName" : {
      "type" : "string",
      "defaultValue" : ""
    },
    "routeTable1Name" : {
      "type" : "string"
    },
    "routeTable2Name" : {
      "type" : "string"
    },
    "routeTable3Name" : {
      "type" : "string"
    },
    "routeTable4Name" : {
      "type" : "string"
    },
    "routeTable5Name" : {
      "type" : "string"
    },
    "routeTable6Name" : {
      "type" : "string"
    },
    "routeTable7Name" : {
      "type" : "string"
    },
    "routeTable8Name" : {
      "type" : "string"
    }
  },
  "variables" : {
    "vnetID" : "[resourceId('Microsoft.Network/virtualNetworks',parameters('virtualNetworkName'))]",
    "subnet1" : "[concat(variables('vnetID'),'/subnets/',parameters('Subnet1Name'))]",
    "subnet2" : "[concat(variables('vnetID'),'/subnets/',parameters('Subnet2Name'))]",
    "subnet3" : "[concat(variables('vnetID'),'/subnets/',parameters('Subnet3Name'))]",
    "subnet4" : "[concat(variables('vnetID'),'/subnets/',parameters('Subnet4Name'))]",
    "subnet5" : "[concat(variables('vnetID'),'/subnets/',parameters('Subnet5Name'))]",
    "subnet6" : "[concat(variables('vnetID'),'/subnets/',parameters('Subnet6Name'))]",
    "subnet7" : "[concat(variables('vnetID'),'/subnets/',parameters('Subnet7Name'))]",
    "subnet8" : "[concat(variables('vnetID'),'/subnets/',parameters('Subnet8Name'))]",
    "routeTable1Id" : "[resourceId('Microsoft.Network/routeTables',parameters('routeTable1Name'))]",
    "routeTable2Id" : "[resourceId('Microsoft.Network/routeTables',parameters('routeTable2Name'))]",
    "routeTable3Id" : "[resourceId('Microsoft.Network/routeTables',parameters('routeTable3Name'))]",
    "routeTable4Id" : "[resourceId('Microsoft.Network/routeTables',parameters('routeTable4Name'))]",
    "routeTable5Id" : "[resourceId('Microsoft.Network/routeTables',parameters('routeTable5Name'))]",
    "routeTable6Id" : "[resourceId('Microsoft.Network/routeTables',parameters('routeTable6Name'))]",
    "routeTable7Id" : "[resourceId('Microsoft.Network/routeTables',parameters('routeTable7Name'))]",
    "routeTable8Id" : "[resourceId('Microsoft.Network/routeTables',parameters('routeTable8Name'))]"
  },
  "resources": [
        {
            "apiVersion": "2015-06-15",
            "type": "Microsoft.Network/virtualNetworks",
            "name": "[parameters('virtualNetworkName')]",
            "location": "[parameters('location')]",
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "[parameters('virtualNetworkAddressPrefix')]"
                    ]
                },
                "subnets": [
                    {
                        "name": "[parameters('Subnet1Name')]",
                        "properties": {
                            "addressPrefix": "[parameters('Subnet1Prefix')]",
                            "routeTable": {
                                "id": "[variables('routeTable1Id')]"
                            }
                        }
                    },
                    {
                        "name": "[parameters('Subnet2Name')]",
                        "properties": {
                            "addressPrefix": "[parameters('Subnet2Prefix')]",
                            "routeTable": {
                                "id": "[variables('routeTable2Id')]"
                            }
                        }
                    },
                    {
                        "name": "[parameters('Subnet3Name')]",
                        "properties": {
                            "addressPrefix": "[parameters('Subnet3Prefix')]",
                            "routeTable": {
                                "id": "[variables('routeTable3Id')]"
                            }
                        }
                    },
                    {
                        "name": "[parameters('Subnet4Name')]",
                        "properties": {
                            "addressPrefix": "[parameters('Subnet4Prefix')]",
                            "routeTable": {
                                "id": "[variables('routeTable4Id')]"
                            }
                        }
                    },
                    {
                        "name": "[parameters('Subnet5Name')]",
                        "properties": {
                            "addressPrefix": "[parameters('Subnet5Prefix')]",
                            "routeTable": {
                                "id": "[variables('routeTable5Id')]"
                            }
                        }
                    },
                    {
                        "name": "[parameters('Subnet6Name')]",
                        "properties": {
                            "addressPrefix": "[parameters('Subnet6Prefix')]",
                            "routeTable": {
                                "id": "[variables('routeTable6Id')]"
                            }
                        }
                    },
                    {
                        "name": "[parameters('Subnet7Name')]",
                        "properties": {
                            "addressPrefix": "[parameters('Subnet7Prefix')]",
                            "routeTable": {
                                "id": "[variables('routeTable7Id')]"
                            }
                        }
                    },
                    {
                        "name": "[parameters('Subnet8Name')]",
                        "properties": {
                            "addressPrefix": "[parameters('Subnet8Prefix')]",
                            "routeTable": {
                                "id": "[variables('routeTable8Id')]"
                            }
                        }
                    }
                ]
            }
        }
  ],
  "outputs": {
    "subnet1Ref": {
      "value": "[variables('subnet1')]",
      "type": "string"
    },
    "subnet2Ref": {
      "value": "[variables('subnet2')]",
      "type": "string"
    },
    "subnet3Ref": {
      "value": "[variables('subnet3')]",
      "type": "string"
    },
    "subnet4Ref": {
      "value": "[variables('subnet4')]",
      "type": "string"
    },
    "subnet5Ref": {
      "value": "[variables('subnet5')]",
      "type": "string"
    },
    "subnet6Ref": {
      "value": "[variables('subnet6')]",
      "type": "string"
    },
    "subnet7Ref": {
      "value": "[variables('subnet7')]",
      "type": "string"
    },
    "subnet8Ref": {
      "value": "[variables('subnet8')]",
      "type": "string"
    } 
  }
}
